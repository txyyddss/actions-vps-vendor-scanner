{
  "http": {
    "timeout_seconds": 35,
    "follow_redirects": true,
    "verify_ssl": true,
    "http2": true,
    "user_agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/142.0.0.0 Safari/537.36",
    "accept_language": "en-US,en;q=0.9"
  },
  "retry": {
    "max_attempts": 3,
    "base_delay_seconds": 1.5,
    "max_delay_seconds": 30,
    "jitter_seconds": 0.5,
    "retry_status_codes": [
      429,
      500,
      502,
      503,
      504
    ]
  },
  "rate_limit": {
    "global_qps": 15,
    "per_domain_qps": 1,
    "default_cooldown_seconds": 45,
    "ratelimit_cooldown_seconds": 90,
    "circuit_breaker_failures": 5,
    "circuit_breaker_cooldown_seconds": 180
  },
  "flaresolverr": {
    "enabled": true,
    "url": "http://127.0.0.1:8191/v1",
    "max_timeout_ms": 180000,
    "session_ttl_minutes": 30,
    "retry_attempts": 3,
    "retry_base_delay_seconds": 2.0,
    "retry_max_delay_seconds": 30.0,
    "retry_jitter_seconds": 0.5,
    "queue_depth_threshold": 15,
    "queue_depth_sleep_seconds": 3.0,
    "reuse_cookies": true,
    "cookie_ttl_seconds": 1800
  },
  "playwright": {
    "enabled": false,
    "headless": true,
    "timeout_ms": 60000,
    "wait_until": "networkidle"
  },
  "proxy": {
    "enabled": false,
    "url": ""
  },
  "scanner": {
    "max_workers": 12,
    "discoverer_max_workers": 12,
    "scan_batch_size": 48,
    "discoverer_max_depth": 3,
    "discoverer_max_pages": 500,
    "stop_tail_window": 60,
    "stop_inactive_streak_category": 20,
    "stop_inactive_streak_product": 60,
    "initial_scan_floor": 20,
    "default_scan_bounds": {
      "whmcs_gid_max": 800,
      "whmcs_pid_max": 3500,
      "hostbill_catid_max": 800,
      "hostbill_pid_max": 4000
    }
  },
  "telegram": {
    "enabled": true,
    "bot_token": "",
    "chat_id": "",
    "topic_id": "",
    "bot_token_env": "TELEGRAM_BOT_TOKEN",
    "chat_id_env": "TELEGRAM_CHAT_ID",
    "topic_id_env": "TELEGRAM_TOPIC_ID",
    "channel_url": "",
    "max_message_length": 4096,
    "max_retries": 5,
    "base_retry_delay": 1.0,
    "min_send_interval": 1.5
  },
  "url_normalizer": {
    "invalid_path_patterns": [
      "contact",
      "contact.php",
      "announcements",
      "announcement",
      "knowledgebase",
      "submitticket",
      "supporttickets",
      "supporttickets.php",
      "clientarea",
      "login",
      "password",
      "pwreset",
      "forgot",
      "register",
      "affiliates"
    ],
    "invalid_extensions": [
      ".png",
      ".jpg",
      ".jpeg",
      ".gif",
      ".webp",
      ".svg",
      ".ico",
      ".css",
      ".js",
      ".woff",
      ".woff2",
      ".ttf",
      ".eot",
      ".pdf",
      ".zip",
      ".tar",
      ".gz"
    ],
    "volatile_query_keys": [
      "sid",
      "session",
      "phpsessid",
      "utm_source",
      "utm_medium",
      "utm_campaign",
      "utm_term",
      "utm_content"
    ],
    "english_language_tags": [
      "en",
      "en-us",
      "en_us",
      "en-gb",
      "en_gb",
      "english"
    ],
    "language_query_keys": [
      "language",
      "lang",
      "locale"
    ],
    "route_query_keys": [
      "rp"
    ]
  },
  "data_merge": {
    "source_priority": {
      "discoverer": 1,
      "category_scanner": 2,
      "product_scanner": 3
    }
  },
  "parsers": {
    "oos_markers": [
      "out of stock",
      "sold out",
      "currently unavailable",
      "is currently unavailable",
      "缺貨中",
      "缺货中",
      "無庫存",
      "无库存"
    ]
  },
  "dashboard": {
    "title": "TX Cicken dashboard",
    "default_theme": "dark",
    "show_stats": true
  },
  "logging": {
    "level": "INFO",
    "json_logs": false
  }
}